#define PIN            4
#define NUMPIXELS      32
Adafruit_NeoPixel pixels = Adafruit_NeoPixel(NUMPIXELS, PIN, NEO_GRB + NEO_KHZ800);

inline uint8_t Color_Interp (uint8_t component, uint8_t index, uint8_t fraction,uint8_t palette){
  return (uint8_t)((uint16_t)((Colors_LUT[0][index][component]<<8)+(Colors_LUT[0][index+1][component]-Colors_LUT[0][index][component])*fraction)>>10);
}

uint32_t getColCamera(uint8_t p_pattern, uint8_t p_pixel,uint16_t p_time) {
  uint32_t rval;
  uint8_t pixval,pixmax,pixoffs,intens,fraction,pixfrac;
  pixval=LED_Patterns[p_pattern][Camera_LUT[p_pixel]]; // OK
  pixmax=(pixval>>4);                                  // OK
  pixoffs=(0x7-((pixval&0xF)>>1))+(p_time>>8);         // OK
  pixfrac=(p_time&0xFF);                               // OK

  intens=((Pulse_LUT[pixoffs]<<8)+(Pulse_LUT[pixoffs+1]-Pulse_LUT[pixoffs])*pixfrac)>>8; //interpolate the pulse intensity

  fraction =(intens*pixmax)&0xFF; // For color interpolation
  intens=(intens*pixmax)>>8;

  rval=pixels.Color(Color_Interp(0,intens,fraction,p_pattern),Color_Interp(1,intens,fraction,p_pattern),Color_Interp(2,intens,fraction,p_pattern));

  if (intens < 1) {
    int rand = random(0, 1000);
    if (rand > 995) rval = rval=pixels.Color(50, 0, 0);
  }

  return rval;
}


void setup()
{
//  Serial.begin(SERIAL,1152000);
  pixels.begin(); // This initializes the NeoPixel library.
  pixels.show(); // This sends the updated pixel color to the hardware.

}


void loop()
{
  for(int i=0;i<NUMPIXELS;i++) {
    pixels.setPixelColor(i, getColCamera((lcount>>11)%N_LED_PATTERNS,i,(lcount<<1)&0xFFF));
  }
  pixels.show(); // This sends the updated pixel color to the hardware.
  lcount++;
}



